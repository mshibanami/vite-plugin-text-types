{
  "name": "vite-plugin-text-types",
  "description": "Vite plugin to generate TypeScript definitions for text assets",
  "version": "1.0.0",
  "type": "module",
  "author": "Manabu Nakazawa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mshibanami/vite-plugin-text-types.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "vite",
    "plugin",
    "typescript",
    "text",
    "assets"
  ],
  "dependencies": {
    "fast-glob": "^3.3.2",
    "picomatch": "^3.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^20.11.0",
    "@types/picomatch": "^3.0.0",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.1"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  }
}
